cc_binary {
    relative_install_path: "hw",
    proprietary: true,

    name: "android.hardware.sensors@2.0-service.kingfisher",
    init_rc: ["android.hardware.sensors@2.0-service.kingfisher.rc"],

    cflags: [
        "-Wall",
        "-Wextra",
        "-Werror"
    ],

    srcs: [
        "service.cpp",
        "Sensors.cpp",
        "IIO_sensor.cpp"
    ],

    shared_libs: [
        "liblog",
        "libfmq",
        "libcutils",
        "libbase",
        "libpower",
        "libutils",
        "libbinder",
        "libhidlbase",
        "libhidltransport",
        "android.hardware.sensors@1.0",
        "android.hardware.sensors@2.0",
    ]
}
